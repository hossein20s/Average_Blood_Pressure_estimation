{
   "data": {
      "columns": [
         "PPG",
         "ABP"
      ],
      "filename": "/tmp/data1.mat",
      "normalise": true,
      "sequence_length": 50,
      "train_test_split": 0.85
   },
   "model": {
      "layers": [
         {
            "input_dim": 1,
            "input_timesteps": 49,
            "neurons": 100,
            "return_seq": true,
            "type": "lstm"
         },
         {
            "rate": 0.2,
            "type": "dropout"
         },
         {
            "neurons": 100,
            "return_seq": true,
            "type": "lstm"
         },
         {
            "neurons": 100,
            "return_seq": false,
            "type": "lstm"
         },
         {
            "rate": 0.2,
            "type": "dropout"
         },
         {
            "activation": "linear",
            "neurons": 1,
            "type": "dense"
         }
      ],
      "loss": "mse",
      "metrics": [
         "acc"
      ],
      "optimizer": "adam",
      "save_dir": "/tmp"
   },
   "training": {
      "batch_size": 32,
      "epochs": 2
   }
}